type Query {
    getAllMovies: [Movie]
    getMovie(id:Int): Movie!
}

type Mutation {
    addMovie(movieRequest: MovieRequest): Movie!
}

type Movie {
    id: ID!
    title: String!
    director: String!
    releaseYear: Int
    movieCast: [String]
}

input MovieRequest {
    title: String!
    director: String!
    releaseYear: Int
    movieCast: [String]
}